{
    "name": "trezo/php-sigep",
    "version": "0.0.1",
    "type": "library",
    "description": "Integração com Web Service do Correios. Gera etiquetas, consulta preços e prazos, imprime etiquetas e PLP, etc.",
    "keywords": ["correios", "sigep"],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev",
            "dev-develop": "1.1-dev"
        }
    },
    "require": {
        "php": ">=5.4",
        "ext-soap": "*",
        "trezo/php-sigep-fpdf": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "PhpSigep": "src/"
        }
    },
    "license": "MIT",
    "repositories": {
        "trezo/php-sigep-fpdf": {
            "type": "git",
            "url": "https://github.com/trezoteam/php-sigep-fpdf.git"
        }
    }
}
